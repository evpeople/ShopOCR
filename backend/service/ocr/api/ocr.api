type (
	OcrReq
	 {
		Base64 string `json:"base64"`
		Url string `json:"url"`
	}
	Position{
		X float64 `json:"x"`
		Y float64 `json:"y"`
	}
	SingleOcrReply {
		Content          string  `json:"content"`
		Confidence    float64 `json:"probability"`
		Pos []Position `json:"pos"`
	}
	OcrReply{
		Code int `json:"code"`
		Msg string `json:"msg"`
		Replies []SingleOcrReply `json:"Replies"`
	}
)
@server(
	jwt: Auth
)
service ocr-api {
	@handler  ocr
	post /ocr (OcrReq) returns (OcrReply)
}